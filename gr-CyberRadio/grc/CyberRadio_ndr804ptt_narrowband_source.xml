<?xml version="1.0"?>
<block>
  <name>[CyberRadio] NDR804-PTT NB IQ Source</name>
  <key>CyberRadio_ndr804ptt_narrowband_source</key>
  <category>[CyberRadio]/Vita and UDP Sources/NDR804</category>
  <import>import CyberRadio</import>
  <make>CyberRadio.ndr804ptt_narrowband_source(
        radioParam=$radioParam, 
        index=$index, 
        idString=$idString,
        enable=$enable, 
        rate=$rate, 
        ddcFreq=$ddcFreq, 
        tunerIndex=$tunerIndex, 
        radioInterface=$radioInterface, 
        dipIndex=$dipIndex, 
        localInterface=$localInterface, 
        udpPort=$udpPort,
        testFilename=$testFilename,
        otherDdcArgs=$otherDdcArgs, 
        )</make>
  <callback>set_ddcFreq($ddcFreq)</callback>
  <callback>set_dipIndex($dipIndex)</callback>
  <callback>set_idString($idString)</callback>
  <callback>set_enable($enable)</callback>
  <callback>set_localInterface($localInterface)</callback>
  <callback>set_otherDdcArgs($otherDdcArgs)</callback>
  <callback>set_radioInterface($radioInterface)</callback>
  <callback>set_radioParam($radioParam)</callback>
  <callback>set_rate($rate)</callback>
  <callback>set_tunerIndex($tunerIndex)</callback>
  <callback>set_udpPort($udpPort)</callback>
  <param>
    <name>Radio Param Dict</name>
    <key>radioParam</key>
    <value>{"type":"ndr804-ptt", "host":"ndr308", "port":8617, "obj":None}</value>
    <type>raw</type>
    <hide>part</hide>
  </param>
  <param>
    <name>Index</name>
    <key>index</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>Enable</name>
    <key>enable</key>
    <value>True</value>
    <type>bool</type>
  </param>
  <param>
    <name>ID String</name>
    <key>idString</key>
    <value>"USER"</value>
    <type>raw</type>
  </param>  
  <param>
    <name>DDC Rate Index</name>
    <key>rate</key>
    <value>0</value>
    <type>int</type>
  </param>
  <param>
    <name>DDC Frequency (Hz)</name>
    <key>ddcFreq</key>
    <value>0.0</value>
    <type>float</type>
  </param>
  <param>
    <name>Tuner Index</name>
    <key>tunerIndex</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>Radio Ethernet Interface Index</name>
    <key>radioInterface</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>DIP Index</name>
    <key>dipIndex</key>
    <value>-1</value>
    <type>int</type>
    <hide>part</hide>
  </param>
  <param>
    <name>Local Ethernet Interface</name>
    <key>localInterface</key>
    <value>"eth6"</value>
    <type>string</type>
  </param>
  <param>
    <name>UDP Port</name>
    <key>udpPort</key>
    <value>11000</value>
    <type>int</type>
  </param>
  <param>
    <name>Test Filename</name>
    <key>testFilename</key>
    <value>""</value>
    <type>raw</type>
  </param>    
  <param>
    <name>Other DDC Args</name>
    <key>otherDdcArgs</key>
    <value>{}</value>
    <type>raw</type>
    <hide>part</hide>
  </param>
  <sink>
    <name>control</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>control</name>
    <type>message</type>
    <optional>1</optional>
  </source>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>1</vlen>
  </source>
</block>
